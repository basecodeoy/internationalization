{
    "name": "basecodeoy/internationalization",
    "description": "A collection of building blocks for internationalization in applications.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "BaseCode Oy",
        "laravel",
        "intl"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/basecodeoy/internationalization",
    "support": {
        "issues": "https://github.com/basecodeoy/internationalization/issues",
        "source": "https://github.com/basecodeoy/internationalization",
        "security": "https://github.com/basecodeoy/internationalization/blob/main/.github/SECURITY.md"
    },
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "archtechx/enums": "^1.1",
        "basecodeoy/datobs": "^1.0",
        "brick/math": "^0.11.0",
        "brick/money": "^0.8.1",
        "brick/phonenumber": "^0.6.0",
        "brick/postcode": "^0.3.3",
        "commerceguys/addressing": "^2.2.3",
        "illuminate/contracts": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0",
        "spatie/laravel-data": "^4.11.1",
        "symfony/intl": "^7.2.0"
    },
    "require-dev": {
        "basecodeoy/standards": "^1.1.1"
    },
    "autoload": {
        "psr-4": {
            "BaseCodeOy\\Internationalization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        },
        "audit": {
            "abandoned": "report"
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
